{
    "presets": [
        [
            "@babel/preset-typescript", {
            "isTSX": true,
            "allExtensions": true
        }
        ],
        [
            "@babel/preset-env", {
              "modules": false
            }
          ],
          "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}